{
  "name": "atlantis-eons-memory-tools",
  "version": "1.0.0",
  "description": "Memory management and context preservation tools for AtlantisEons Unreal Engine project",
  "main": "memory-manager.js",
  "scripts": {
    "memory": "node memory-cli.js",
    "memory-server": "node memory-mcp-server.js",
    "analyzer": "node unreal-code-analyzer.js",
    "mcp": "node dynamic-mcp-server.js",
    "start": "npm run memory-server",
    "init": "node memory-cli.js init",
    "stats": "node memory-cli.js stats detail",
    "export": "node memory-cli.js export",
    "search": "node memory-cli.js search"
  },
  "bin": {
    "atlantis-memory": "./memory-cli.js",
    "atlantis-analyzer": "./unreal-code-analyzer.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "unreal-engine",
    "memory-management",
    "context-preservation",
    "mcp",
    "code-analysis",
    "game-development"
  ],
  "author": "AtlantisEons Development Team",
  "license": "MIT"
}
