{
  "name": "atlantis-eons-memory-tools",
  "version": "1.0.0",
  "description": "Memory management and context preservation tools for AtlantisEons Unreal Engine project",
  "main": "memory-manager.js",
  "scripts": {
    "memory": "node scripts/memory-cli.js",
    "memory-server": "node scripts/memory-mcp-server.js",
    "analyzer": "node scripts/unreal-code-analyzer.js",
    "mcp": "node scripts/dynamic-mcp-server.js",
    "start": "npm run memory-server",
    "init": "node scripts/memory-cli.js init",
    "stats": "node scripts/memory-cli.js stats detail",
    "export": "node scripts/memory-cli.js export",
    "search": "node scripts/memory-cli.js search"
  },
  "bin": {
    "atlantis-memory": "./scripts/memory-cli.js",
    "atlantis-analyzer": "./scripts/unreal-code-analyzer.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "unreal-engine",
    "memory-management",
    "context-preservation",
    "mcp",
    "code-analysis",
    "game-development"
  ],
  "author": "AtlantisEons Development Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.0"
  }
}
